   "source": [
    "s = input()\n",
    "t=s.count('1')\n",
    "if t%2:\n",
    "    print(\"NO\")\n",
    "else:\n",
    "    t=t/2\n",
    "    j=0\n",
    "    for i in range(len(s)):\n",
    "        if s[i]=='1':\n",
    "            t-=1\n",
    "        if t==0:\n",
    "            j=i\n",
    "            break\n",
    "    print(s[0:j+1]+' '+s[j+1:len(s)])"
   ]
